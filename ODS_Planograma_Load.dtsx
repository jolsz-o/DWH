                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
 T.[PLN_ID] AS PlanogramId
,RIGHT('0' + PS.StoreId,2) AS StoreId
,convert(smalldatetime,Replace(PP.StartDate,'.','-'),103) AS StartDate
,cast(Isnull(case when convert(datetime, convert(smalldatetime,Replace(PP.EndDate,'.','-'),103)) = '1900-01-01 00:00:00' THEN convert(varchar(10), getdate(), 23)+' 00:00:00'  else convert(varchar(19), convert(smalldatetime,Replace(PP.EndDate,'.','-'),103),20) END, '') as smalldatetime) EndDate
,F.FIXEL_ID AS FixelId
,P.POSITION_ID AS PositionId
,L.LOCATION_ID AS LocationId
--,TU.TOTAL_UNITS
,PR.[NR_CEN] AS ItemId 
,T.SUM_DEPTH_FACINGS AS DepthFacing
,T.MAX_TOTAL_FACINGS  AS TotalFacing 
, T.SUM_TOTAL_UNITS   AS TotalUnits
,(T.MAX_TOTAL_FACINGS*PR.WIDTH)/100   AS TotalLengthMeters
,T.SUM_HORIZ_SPACE/100   AS TotalLengthMeters2

FROM
(
SELECT  C.[PLN_ID]
       ,C.[PRODUCT_ID]
	,D.MAX_TOTAL_FACINGS
,SUM(C.TOTAL_FACINGS) AS SUM_TOTAL_FACINGS
,SUM(C.[TOTAL_UNITS]) AS SUM_TOTAL_UNITS
,SUM(C.[DEPTH_FACINGS] ) AS SUM_DEPTH_FACINGS 
,SUM(C.[HORIZ_SPACE]) AS SUM_HORIZ_SPACE

 FROM ODS_Planogram_Position C

 INNER JOIN (
SELECT DISTINCT A.[PLN_ID]
               ,A.[PRODUCT_ID]
               ,B.MAX_TOTAL_FACINGS AS MAX_TOTAL_FACINGS
               FROM ODS_Planogram_Position A
INNER JOIN (
SELECT [PLN_ID]
      ,[PRODUCT_ID]
      ,MAX([TOTAL_FACINGS])  AS MAX_TOTAL_FACINGS
       FROM ODS_Planogram_Position
       GROUP BY [PLN_ID],[PRODUCT_ID]
	HAVING COUNT(*)&gt;1) B
	 
      ON A.PLN_ID=B.PLN_ID AND A.PRODUCT_ID=B.PRODUCT_ID
      WHERE A.[TOTAL_FACINGS]=B.MAX_TOTAL_FACINGS) D
	
	ON C.PLN_ID=D.PLN_ID AND C.PRODUCT_ID=D.PRODUCT_ID  
	GROUP BY   C.[PLN_ID],C.[PRODUCT_ID],D.MAX_TOTAL_FACINGS) T

	LEFT JOIN ODS_Planogram_PlnStoreConnect PS
	ON T.PLN_ID=PS.PLN_ID
	LEFT JOIN ODS_Planogram_Planogram PP
	ON T.PLN_ID=PP.PLN_ID
	LEFT JOIN ODS_Planogram_Products PR
    On T.PRODUCT_ID= PR.PRODUCT_ID

	
	OUTER APPLY(SELECT TOP 1 [FIXEL_ID]    FROM ODS_Planogram_Position WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID AND TOTAL_FACINGS=T.MAX_TOTAL_FACINGS)  F
	OUTER APPLY(SELECT TOP 1 [POSITION_ID] FROM ODS_Planogram_Position WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID AND TOTAL_FACINGS=T.MAX_TOTAL_FACINGS)  P
	OUTER APPLY(SELECT TOP 1 [LOCATION_ID] FROM ODS_Planogram_Position WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID AND TOTAL_FACINGS=T.MAX_TOTAL_FACINGS)  L
	OUTER APPLY(SELECT TOP 1 [TOTAL_UNITS] FROM ODS_Planogram_Position WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID AND TOTAL_FACINGS=T.MAX_TOTAL_FACINGS)  TU
	
Where not ISNULL(PP.StartDate,'') = ''
And year(convert(smalldatetime,Replace(PP.StartDate,'.','-'),103))= 2019
and StartDate &lt;&gt; '01-04-202 '
AND ((ISNULL(ENDDATE,'')='' OR CONVERT(INT,RIGHT(PP.ENDDATE,4))&gt;=2019 OR CONVERT(INT,RIGHT(ENDDATE,4))=1900))
			

UNION ALL

SELECT 
       T.[PLN_ID] AS PlanogramId
     ,RIGHT('0' + PS.StoreId,2) AS StoreId
 	 ,convert(smalldatetime,Replace(PP.StartDate,'.','-'),103)  AS StartDate
    , cast(Isnull(case when convert(datetime, convert(smalldatetime,Replace(PP.EndDate,'.','-'),103)) = '1900-01-01 00:00:00' THEN convert(varchar(10), getdate(), 23)+' 00:00:00'  else convert(varchar(19), convert(smalldatetime,Replace(PP.EndDate,'.','-'),103),20) END, '') as smalldatetime) EndDate
       ,F.[FIXEL_ID] AS FixelId
      ,P.[POSITION_ID]  AS PositionId
      ,L.[LOCATION_ID] AS LocationId
      ,PR.[NR_CEN] AS ItemId
	  ,[DEPTH_FACINGS] AS DepthFacing
	  ,[TOTAL_FACINGS] AS TotalFacing
      ,TU.[TOTAL_UNITS] AS TotalUnits
      ,(T.TOTAL_FACINGS*PR.WIDTH)/100   AS TotalLengthMeters
      ,T.[HORIZ_SPACE]/100   AS TotalLengthMeters2

FROM ODS_Planogram_Position T

LEFT JOIN ODS_Planogram_PlnStoreConnect PS
ON T.PLN_ID=PS.PLN_ID
LEFT JOIN ODS_Planogram_Planogram PP
ON T.PLN_ID=PP.PLN_ID
LEFT JOIN ODS_Planogram_Products PR
ON T.PRODUCT_ID= PR.PRODUCT_ID

OUTER APPLY(SELECT     [FIXEL_ID]    FROM ODS_Planogram_Position WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID )  F
	OUTER APPLY(SELECT     [POSITION_ID] FROM ODS_Planogram_Position  WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID )  P
	OUTER APPLY(SELECT     [LOCATION_ID] FROM ODS_Planogram_Position WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID )  L
	OUTER APPLY(SELECT     [TOTAL_UNITS] FROM ODS_Planogram_Position WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID )  TU

Where not ISNULL(PP.StartDate,'') = ''
And year(convert(smalldatetime,Replace(PP.StartDate,'.','-'),103)) &gt;= 2019
and StartDate &lt;&gt; '01-04-202 '
AND ((ISNULL(ENDDATE,'')='' OR CONVERT(INT,RIGHT(PP.ENDDATE,4))&gt;=2019 OR CONVERT(INT,RIGHT(ENDDATE,4))=1900))
GROUP BY
       
       T.[PLN_ID] 
     ,PS.StoreId 
 	 ,PP.StartDate
     ,PP.EndDate
        ,F.[FIXEL_ID]
      ,P.[POSITION_ID]  
      ,L.[LOCATION_ID] 
      ,PR.[NR_CEN]
  ,[DEPTH_FACINGS] 
  ,[TOTAL_FACINGS] 
      ,TU.[TOTAL_UNITS]
	  ,(T.TOTAL_FACINGS*PR.WIDTH)/100
      ,T.[HORIZ_SPACE]/100 
      
	HAVING COUNT(*)=1
		
 --ORDER BY 
 --PS.StoreId,
    -- T.PLN_ID
    --,T.PRODUCT_ID</property>
