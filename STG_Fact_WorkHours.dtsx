      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{30B5E175-D822-4E25-A714-0E6FA4D7D42D}"
          SQLTask:SqlStatementSource="select dtDate,&#xA;&#x9;&#x9;StoreId,&#xA;&#x9;&#x9;DepartmentId,&#xA;&#x9;&#x9;EmployeeId,&#xA;&#x9;&#x9;EventType,&#xA;&#x9;&#x9;INVOICE_TIME_ID,&#xA; case when datepart(&quot;Hour&quot;,convert(time, right('0' + case when INVOICE_TIME_id = 0 then '00:00' else INVOICE_TIME_DESC end + ':00',8),8)) &gt; datepart(&quot;Hour&quot;,StartTime) &#xA; then case when datepart(&quot;Hour&quot;,convert(time, right('0' + case when INVOICE_TIME_id = 0 then '00:00' else INVOICE_TIME_DESC end + ':00',8),8)) &lt; DATEPART(&quot;Hour&quot;,EndTime)&#xA; Then 60 else datediff(&quot;Minute&quot;,convert(time, right('0' + case when INVOICE_TIME_id = 0 then '00:00' else INVOICE_TIME_DESC end + ':00',8),8),EndTime) end&#xA; Else case when datepart(&quot;Hour&quot;,convert(time, right('0' + case when INVOICE_TIME_id = 0 then '00:00' else INVOICE_TIME_DESC end + ':00',8),8)) = DATEPART(&quot;Hour&quot;,EndTime)&#xA; then datediff(MINUTE ,StartTime,EndTime)&#xA; Else 60 - DATEPART(&quot;Minute&quot;,Starttime)End &#xA; End WorkingMin,&#xA; 2 as SystemId &#xA; Into #TT&#xA; FROM dbo.STG_Fact_RCPCWorkHours&#xA; , DWH_DIM_Time&#xA; where YEAR(dtDate) &gt;= 2013 &#xA; and datepart(&quot;Hour&quot;,CONVERT(time, right('0' + case when INVOICE_TIME_id = 0 then '00:00' else INVOICE_TIME_DESC end + ':00',8),8))&#xA; between datepart(&quot;Hour&quot;,startTime) and datepart(&quot;Hour&quot;,EndTime)&#xA; &#xA; --Now we will add missing data in the new HR system from RCP&#xA; Insert Into STG_Fact_WorkHours &#xA; Select * From #TT&#xA; Where not exists (select * &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;from STG_Fact_WorkHours &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;Where STG_Fact_WorkHours.dtDate = #TT.dtDate&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;And STG_Fact_WorkHours.StoreId = #TT.StoreId&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;--And STG_Fact_WorkHours.DepartmentId = #TT.DepartmentId&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;And STG_Fact_WorkHours.EmployeeId = #TT.EmployeeId&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;--And STG_Fact_WorkHours.EventType = #TT.EventType&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;And STG_Fact_WorkHours.HourId = #TT.INVOICE_TIME_ID)&#xA;&#xA;Drop Table #TT" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
