        <SQLTask:SqlTaskData
          SQLTask:Connection="{30B5E175-D822-4E25-A714-0E6FA4D7D42D}"
          SQLTask:SqlStatementSource="Update STG_Fact_RCPCWorkHours Set&#xA;&#x9;EndTime = TT.EventTime&#xA;From (Select   i.DATA_OP,i.ID_SKLEP,i.KOD_UZYT,KOD,convert(int,case when charindex('/',ID) &gt; 0 then LEFT(ID,len(ID)-3) else ID end) EventId,replace(max(i.GODZ_OP),'24:','00:') EventTime&#xA;&#x9;  From dbo.ODS_RCP i with(nolock)&#xA;&#x9;  Where  Year(DATA_OP) &gt; 2011 and DATA_OP &lt; getdate() and KOD = 'P_K' and not GODZ_OP is null&#xA;&#x9;  Group by  i.DATA_OP,i.ID_SKLEP,i.KOD_UZYT,KOD, convert(int,case when charindex('/',ID) &gt; 0 then LEFT(ID,len(ID)-3) else ID end)) TT&#xA;Where STG_Fact_RCPCWorkHours.dtDate = TT.DATA_OP&#xA;  And STG_Fact_RCPCWorkHours.StoreId = TT.ID_SKLEP&#xA;  And STG_Fact_RCPCWorkHours.EmployeeId = TT.KOD_UZYT&#xA;  And STG_Fact_RCPCWorkHours.EventType = 'P_P'&#xA;  And STG_Fact_RCPCWorkHours.EventId &lt; TT.EventId " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
