                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select  [date] dtDate, 
		right('0' + cast(shop as varchar(3)),2) StoreId,
		department,
		Employee_ID EmployeeId,
		Case when is_break =1 then 'P_P' else 'POC' end EventType,
		INVOICE_TIME_ID,
 case when datepart("Hour",convert(time, right('0' + case when INVOICE_TIME_id = 0 then '00:00' else INVOICE_TIME_DESC end + ':00',8),8)) &gt; datepart("Hour",[time]) 
 then case when datepart("Hour",convert(time, right('0' + case when INVOICE_TIME_id = 0 then '00:00' else INVOICE_TIME_DESC end + ':00',8),8)) &lt; DATEPART("Hour",EndTime)
 Then 60 else datediff("Minute",convert(time, right('0' + case when INVOICE_TIME_id = 0 then '00:00' else INVOICE_TIME_DESC end + ':00',8),8),EndTime) end
 Else case when datepart("Hour",convert(time, right('0' + case when INVOICE_TIME_id = 0 then '00:00' else INVOICE_TIME_DESC end + ':00',8),8)) = DATEPART("Hour",EndTime)
 then datediff(MINUTE ,[time],EndTime)
 Else 60 - DATEPART("Minute",[time])End 
 End WorkingMin,
 1 as SystemId 
 FROM [SuperPharmPoland].[dbo].[STG_Fact_HRWorkHours]
 , DWH_DIM_Time
 where datepart("Hour",CONVERT(time, right('0' + case when INVOICE_TIME_id = 0 then '00:00' else INVOICE_TIME_DESC end + ':00',8),8))
 between datepart("Hour",[Time]) and datepart("Hour",EndTime)</property>
