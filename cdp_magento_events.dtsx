              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="ADO NET abandoned_carts"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT /*+ MAX_EXECUTION_TIME(100000000) */
	cast(updated_at as date) as updated_at,
    `main_table`.entity_id,
    `main_table`.customer_id, 
    cast(`main_table`.customer_email as char(200)) as customer_email,
    cast(`main_table`.customer_firstname as char(200)) as customer_firstname,
    cast(`main_table`.customer_lastname as char(200)) as customer_lastname,
    `main_table`.customer_is_guest,
    `main_table`.customer_gender, 
    `main_table`.abandoned_cart_email_status,
    `main_table`.items_count,
    `main_table`.base_currency_code,
    `main_table`.grand_total,
    (main_table.base_subtotal_with_discount * main_table.base_to_global_rate) AS `subtotal`
FROM
    `quote` AS `main_table`
WHERE
    (`items_count` != '0')
        AND (main_table.is_active = '1')
        AND (main_table.customer_id != '')
        AND (store_id IN (2 , 4, 5)) -- Drugstore PL, Pharmacy PL, Italy
		
        and updated_at &gt;= DATE_SUB(cast(CURDATE() as date), INTERVAL 30 DAY)	
;</property>
