                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT /*+ MAX_EXECUTION_TIME(100000000000000) */
    `main_table`.`qty` AS `item_qty`,
    cast(`main_table`.`added_at` as date) as added_at,
    `main_table`.`product_id`,
    cast(IFNULL(name, 'Wish List') as char(100)) AS `wishlist_name`,
    cast((CONCAT(IF(customer.prefix IS NOT NULL
                    AND customer.prefix != '',
                CONCAT(LTRIM(RTRIM(customer.prefix)), ' '),
                ''),
            LTRIM(RTRIM(customer.firstname)),
            ' ',
            IF(customer.middlename IS NOT NULL
                    AND customer.middlename != '',
                CONCAT(LTRIM(RTRIM(customer.middlename)), ' '),
                ''),
            LTRIM(RTRIM(customer.lastname)),
            IF(customer.suffix IS NOT NULL
                    AND customer.suffix != '',
                CONCAT(' ', LTRIM(RTRIM(customer.suffix))),
                ''))) as char(100)) AS `customer_name`
FROM
    `wishlist_item` AS `main_table`
        INNER JOIN
    `wishlist` AS `wishlist_table` ON main_table.wishlist_id = wishlist_table.wishlist_id
        LEFT JOIN
    `customer_entity` AS `customer` ON customer.entity_id = wishlist_table.customer_id
WHERE
    (`main_table`.`store_id` IN (2 , 4, 5)) -- Drugstore PL, Pharmacy PL, Italy
    and cast(`main_table`.`added_at` as date) &gt;= DATE_SUB(cast(CURDATE() as date), INTERVAL 1 YEAR)
;</property>
