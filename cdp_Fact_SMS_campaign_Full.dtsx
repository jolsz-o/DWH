                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">with unique_list as (
						select distinct --results
							   [numer_komórki] as [phone number]
							  ,[ID_kampanii] as [campaign_id]
						from [cdp].[dbo].[Redlink_sms_results]
						where CAST(left([data_wysyłki_kampanii],10) AS date) &gt;= CAST(GETDATE()-10 AS date)
						union
						select distinct --clicks
							   [numer_komórki] as [phone number]
							  ,[ID_kampanii] as [campaign_id]
						from [cdp].[dbo].[Redlink_sms_clicks]
						where CAST(left([data_kliknięcia],10) AS date) &gt;= CAST(GETDATE()-10 AS date)
				     )
SELECT 
       [numer_komórki] as [phone number]
      ,[ID_kampanii] as [campaign_ID]
      ,[nazwa_kampanii] as [campaign_name]
      ,[data_wysyłki_kampanii] as [campaign_ship_date]
      ,[data_kliknięcia] as [click_date]
      ,[link]
      ,[FileName]
FROM [CDP].[dbo].[Redlink_sms_clicks] a
inner join unique_list b
on a.[numer_komórki] = b.[phone number] and a.[ID_kampanii] = b.campaign_id</property>
