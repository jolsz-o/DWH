      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{971E352C-0DF7-4793-AC47-1867A5A81571}"
          SQLTask:SqlStatementSource="Truncate Table DW_Fact_Planograms&#xA;&#xA;Insert Into DW_Fact_Planograms&#xA;Select P.PLN_ID PlanogramId,right('0' + PS.StoreId,2) StoreId, convert(smalldatetime,Replace(PL.StartDate,'.','-'),103) StartDate, &#xA;&#x9;Convert(smalldatetime,PL.EndDate,103) EndDate, &#xA;&#x9;F.FIXEL_ID FixelId,&#xA;&#x9;P.POSITION_ID PositionId, P.LOCATION_ID LocationId, PP.NR_CEN ItemId,&#xA;&#x9;P.DEPTH_FACINGS DepthFacing, P.TOTAL_FACINGS TotalFacing, P.TOTAL_UNITS TotalUnits,&#xA;(P.TOTAL_FACINGS * WIDTH)/100 as TotalLenghtMeters,&#xA;HORIZ_SPACE/100 as TotalLenghtMeters2&#xA;From dbo.ODS_Planogram_Position P&#xA;Left Join dbo.ODS_Planogram_Fixel F&#xA;On P.PLN_ID = F.PLN_ID&#xA;And P.FIXEL_ID = F.FIXEL_ID&#xA;Left Join dbo.ODS_Planogram_Planogram PL&#xA;On P.PLN_ID = PL.PLN_ID&#xA;Left Join dbo.ODS_Planogram_PlnStoreConnect PS&#xA;on P.PLN_ID = PS.PLN_ID&#xA;Left Join dbo.ODS_Planogram_Products PP&#xA;On P.PRODUCT_ID = PP.PRODUCT_ID&#xA;Where not ISNULL(PL.StartDate,'') = ''&#xA;And RIGHT(PL.StartDate,4) &gt;= 2015&#xA;Order by startdate desc" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
