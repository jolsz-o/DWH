              refId="Package\1st_load  -    cdp_ODS_Freshdesk_tickets + other calculations over dwh --&gt; cdp_STG_DIM_customer_KPIs\DWH_Invoice_Fact 365d"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="DWH_Invoice_Fact 365d"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- historic
with kpi_1 as (
				select 
					   LSClientId,
					   cast(isnull(sum(case when year(INVOICE_DATE) = year(getdate()) then [Net_Revenue] end),0) as numeric(20,2))              [Net_Revenue_Current_Year],
					   cast(isnull(sum(case when INVOICE_DATE &gt;= dateadd(month,-3,getdate()) then [Net_Revenue] end),0) as numeric(20,2))       [Net_Revenue_Last_3M],
					   cast(isnull(sum(case when INVOICE_DATE &gt;= dateadd(month,-6,getdate()) then [Net_Revenue] end),0) as numeric(20,2))       [Net_Revenue_Last_6M],
					   isnull(count(distinct case when year(INVOICE_DATE) = year(getdate()) then INVOICE_ID end),0)                             [inv_count_current_year],
					   isnull(count(distinct case when INVOICE_DATE &gt;= dateadd(month,-6,getdate()) then INVOICE_ID end),0)                      [inv_count_last_6M],
					   isnull(count(distinct case when INVOICE_DATE &gt;= dateadd(month,-3,getdate()) then INVOICE_ID end),0)                      [inv_count_last_3M],
				       cast(isnull(sum(case when year(INVOICE_DATE) = year(getdate())-1 then [Net_Revenue] end),0) as numeric(20,2))            [Net_Revenue_Last_Year],
	                   isnull(count(distinct case when year(INVOICE_DATE) = year(getdate())-1 then INVOICE_ID+STORE_ID end),0)                  [inv_count_last_year],
					   max(INVOICE_DATE) as																						                [last_transaction_date],
					   -- diff from inc. !!!! ----
					   cast(isnull(sum([Net_Revenue]),0) as numeric(20,2))															            [Net_Revenue_Life_Time],
