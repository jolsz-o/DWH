                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
       [ls_id] as [Loyalty Id]
	  ,count(prom_id) as #_sent_cupons_lt
	  ,isnull(count(case when year([coupon_sent_date]) = year(getdate()) then prom_id end),0) as [#_sent_cupons_current_year]
	  ,isnull(count(case when year([coupon_sent_date]) = year(getdate())-1 then prom_id end),0) as [#_sent_cupons_last_year]
	  ,isnull(count(case when [coupon_sent_date]&gt;= dateadd(month,-6,getdate()) then prom_id end),0) as [#_sent_cupons_last_6m]
	  ,isnull(count(case when [coupon_sent_date]&gt;= dateadd(month,-3,getdate()) then prom_id end),0) as [#_sent_cupons_last_3m]
	  ,count([invoice_id]) as #_redemed_cupons_lt
	  ,isnull(count(case when year([inv_date]) = year(getdate()) then [invoice_id] end),0) as [#_redemed_cupons_current_year]
	  ,isnull(count(case when year([inv_date]) = year(getdate())-1 then [invoice_id] end),0) as [#_redemed_cupons_last_year]
	  ,isnull(count(case when [inv_date]&gt;= dateadd(month,-6,getdate()) then [invoice_id] end),0) as [#_redemed_cupons_last_6m]
	  ,isnull(count(case when [inv_date]&gt;= dateadd(month,-3,getdate()) then [invoice_id] end),0) as [#_redemed_cupons_last_3m]
from (
		select
			   s.[prom_id]
			  ,s.[ls_id]
			  ,s.[coupon_sent_date]
			  ,s.[start_activation_date]
			  ,s.[end_activation_date]
			  ,a.[invoice_id]
			  ,a.store_id
			  ,a.inv_date
		from [superpharmpoland].[dbo].[dwh_fact_sent_civalue_coupons] s
		left join (
					SELECT 
						   a.[invoice_id]
						  ,a.[store_id]
						  ,a.[line_number]
						  ,a.[promotion_id]
						  ,a.[price_before_current_promotion_discount]
						  ,a.[promotion_discount]
						  ,a.[price_after_current_promotion_discount]
						  ,a.[item_id]
						  ,a.[inv_date]
						  ,a.[ls_kod]
						  ,a.[ID]
						  ,ls.Card_Number as ls_id
					FROM [SuperPharmPoland].[dbo].[DWH_fact_activated_promotions] a
					left join [SuperPharmPoland].[dbo].[cdp_ODS_LS_customers_SapakimDB] ls
					on a.ls_kod = ls.kod_ls
				   ) a
		on s.prom_id = a.promotion_id and s.ls_id = a.ls_id
		) sent_activ
group by [ls_id]
;</property>
