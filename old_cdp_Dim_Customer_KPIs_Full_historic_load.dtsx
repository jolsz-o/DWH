                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- historic
select 
		[customer_email],
		cast(isnull(sum(case when year(created_at) = year(getdate()) then [gross_revenue] end),0) as numeric(20,2))                    [gross_Revenue_Current_Year_online],
		cast(isnull(sum(case when year(created_at) = year(getdate())-1 then [gross_revenue] end),0) as numeric(20,2))                  [gross_Revenue_Last_Year_online],
		cast(isnull(sum(case when created_at &gt;= dateadd(month,-3,getdate()) then [gross_revenue] end),0) as numeric(20,2))             [gross_Revenue_Last_3M_online],
		cast(isnull(sum(case when created_at &gt;= dateadd(month,-6,getdate()) then [gross_revenue] end),0) as numeric(20,2))             [gross_Revenue_Last_6M_online],
		isnull(count(distinct case when year(created_at) = year(getdate()) then [entity_id] end),0)                                 [order_count_current_year_online],
        isnull(count(distinct case when year(created_at) = year(getdate())-1 then [entity_id] end),0)                               [order_count_last_year_online],
		isnull(count(distinct case when created_at &gt;= dateadd(month,-6,getdate()) then [entity_id] end),0)                          [order_count_last_6M_online],
		isnull(count(distinct case when created_at &gt;= dateadd(month,-3,getdate()) then [entity_id] end),0)                          [order_count_last_3M_online],
		max(created_at)																									            [last_order_date_online],
		-- diff from inc. !!!! ----
		cast(isnull(sum([gross_revenue]),0) as numeric(20,2))									                                        [gross_Revenue_Life_Time_online],
		isnull(count(distinct [entity_id]),0)                                                                                       [order_count_Life_Time_online]
		---------------------------
from [cdp_DWH_Fact_sales_order_magento]

