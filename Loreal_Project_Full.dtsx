              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select [order_date], [SKU], [is_pickup], [store_city_and_desc],
       min([revenue_after_disc]) as min_product_price,
       sum([quantity]) as count_units_sold
from (
		select 
			   cast(ord.[invoice_date] as date) as [order_date] -- by mistake named as "invoice_date" in pipeline
			  ,case 
				  when (ord.[quickpickup] = 0 or ord.[quickpickup] is null) and (ord.[iddeliveryshop]=0 or ord.[iddeliveryshop] is null) then 0
				  else 1
			   end as [is_pickup]
			  ,case 
				 when st.[City] is not null and st.[Store_Desc] is not null then concat(st.[City], ' | ', st.[Store_Desc])
				 else 'none'
			   end as [store_city_and_desc]
			  ,it.[Barcode_ID] as SKU
			  ,ord.[revenue_after_disc]
			  ,ord.[quantity]
		from [dbo].[DWH_Fact_eComm_Sales_Loreal] ord
		left join [dbo].[DWH_DIM_Stores] st on st.[Store_ID] = ord.[iddeliveryshop]
		left join [dbo].[DWH_DIM_Items] it on it.[Item_ID] = ord.[item_id]
		where (lower(it.Brand_Desc) like '%loreal%' or lower(it.MainSupplier_Desc) like '%loreal%')
		  and ((lower(it.Brand_Desc) like '%roche%' and lower(it.Brand_Desc) not like '%guy laroche%')
				or lower(it.Brand_Desc) like '%cerave%'
				or lower(it.Brand_Desc) like '%vichy%')
     ) row_data
group by [order_date], [SKU], [is_pickup], [store_city_and_desc]
;</property>
