                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">declare 
@MinDate datetime,
@MaxDate datetime

Select @MinDate = MIN(INVOICE_DATE), @MaxDate = case when MAX(INVOICE_DATE) &gt; getdate() then getdate() else Max(Invoice_date) end
From STG_Invoice_Fact

Select A.Invoice_date,A.InvoiceIdnt ,A.INVOICE_ROW_NUMBER , A.ITEM_ID as ItemId1, B.ITEM_ID as ItemId2
From dbo.DWH_Invoice_Fact A
	INNER JOIN dbo.DWH_Invoice_Fact B
	ON A.InvoiceIdnt  = B.InvoiceIdnt  
	AND A.ITEM_ID  &lt;&gt; B.ITEM_ID
Where A.Invoice_date between @MinDate and @MaxDate 
And B.Invoice_date between @MinDate and @MaxDate</property>
