                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
	   inv_prom.[NR_OPER] as invoice_id 
      ,inv_prom.[ID_SKLEP] as store_id 
      ,inv_prom.[Lp] as line_number 
      ,inv_prom.[ID_PROM] as promotion_id 
      ,inv_prom.[CENA_PRZED] as price_before_current_promotion_discount
      ,inv_prom.[Korzysc] as promotion_discount
      ,inv_prom.[CENA] as price_after_current_promotion_discount
