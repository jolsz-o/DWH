                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
	   inv_prom.[NR_OPER] as invoice_id 
      ,inv_prom.[ID_SKLEP] as store_id 
      ,inv_prom.[Lp] as line_number 
      ,inv_prom.[ID_PROM] as promotion_id 
      ,inv_prom.[CENA_PRZED] as price_before_current_promotion_discount
      ,inv_prom.[Korzysc] as promotion_discount
      ,inv_prom.[CENA] as price_after_current_promotion_discount
      ,inv_prom.[NR_CEN] as item_id
	  ,inv_headline.DATA_OP as inv_date
	  ,inv_headline.NR_KONTR as ls_kod
	  ,inv_prom.ID
from [OPER_PROM] (nolock) inv_prom
left join OPER (nolock) inv_headline on inv_headline.NR_OPER = inv_prom.NR_OPER
								    and inv_headline.ID_SKLEP = inv_prom.ID_SKLEP
-- where cast(inv_headline.DATA_OP as date) &gt;= '2023-06-01'
where cast(inv_headline.DATA_OP as date) between '2022-01-01' and '2023-05-31'
;</property>
