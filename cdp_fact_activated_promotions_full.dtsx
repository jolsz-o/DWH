	  ,inv_headline.DATA_OP as inv_date
	  ,inv_headline.NR_KONTR as ls_kod
	  ,inv_prom.ID
from [OPER_PROM] (nolock) inv_prom
left join OPER (nolock) inv_headline on inv_headline.NR_OPER = inv_prom.NR_OPER
								    and inv_headline.ID_SKLEP = inv_prom.ID_SKLEP
where cast(inv_headline.DATA_OP as date) &gt;= '2023-06-01'
;</property>
