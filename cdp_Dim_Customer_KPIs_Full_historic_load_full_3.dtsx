                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- 1 ) Freshdesk tickets calculations --
----------------------------------------

-- historic / inc.
select trim(lower([requester_email])) as [requester_email],
       count(distinct id) #_life_time_complaints,
	   count(distinct 
	         case 
	            when year(cast([created_at] as date)) = year(getdate())
				    then id
			 end) #_complaints_current_year,    
	   count(distinct 
	         case 
	            when year(cast([created_at] as date)) = year(getdate())-1
				    then id
			 end) #_complaints_last_year,
	   count(distinct 
	         case 
	            when [status] in (2,3)
				    then id
			 end) #_open_complaints,
	   count(distinct 
	         case 
	            when [status] in (2,3) and cast([created_at] as date) &lt;= getdate()-7
				    then id
			 end) #_open_complaints_more_then_7D,
	   count(distinct 
	         case 
	            when [status] in (2,3) and [is_escalated] = 'True'
				    then id
			 end) #_open_escalated_complaints,
	   count(distinct 
	         case 
	            when year(cast([created_at] as date)) = year(getdate())-1 and
				     month(cast([created_at] as date)) &lt;= month(getdate()) and
					 day(cast([created_at] as date)) &lt;= day(getdate()) 
				    then id
			 end) #_complaints_last_year_ytd

from [SuperPharmPoland].[dbo].[cdp_ODS_Freshdesk_tickets]
where spam = 'False'
group by [requester_email]
;</property>
