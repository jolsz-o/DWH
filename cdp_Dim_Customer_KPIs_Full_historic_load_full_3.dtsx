                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- replaced source data from dwh_invoice_fact to vW_Fact_Invoice: 2024-05-21

with kpi_1 as (
				select 
					   LSClientId,
					   cast(isnull(sum(case when year(INVOICE_DATE) = year(getdate()) then [Net_Revenue] end),0) as numeric(20,2))              [Net_Revenue_Current_Year],
					   cast(isnull(sum(case when INVOICE_DATE &gt;= dateadd(month,-3,getdate()) then [Net_Revenue] end),0) as numeric(20,2))       [Net_Revenue_Last_3M],
					   cast(isnull(sum(case when INVOICE_DATE &gt;= dateadd(month,-6,getdate()) then [Net_Revenue] end),0) as numeric(20,2))       [Net_Revenue_Last_6M],
					   isnull(count(distinct case when year(INVOICE_DATE) = year(getdate()) then INVOICE_ID+STORE_ID end),0)                             [inv_count_current_year],
					   isnull(count(distinct case when INVOICE_DATE &gt;= dateadd(month,-6,getdate()) then INVOICE_ID+STORE_ID end),0)                      [inv_count_last_6M],
					   isnull(count(distinct case when INVOICE_DATE &gt;= dateadd(month,-3,getdate()) then INVOICE_ID+STORE_ID end),0)                      [inv_count_last_3M],
				       cast(isnull(sum(case when year(INVOICE_DATE) = year(getdate())-1 then [Net_Revenue] end),0) as numeric(20,2))            [Net_Revenue_Last_Year],
	                   isnull(count(distinct case when year(INVOICE_DATE) = year(getdate())-1 then INVOICE_ID+STORE_ID end),0)                  [inv_count_last_year],
					   max(INVOICE_DATE) as																						                [last_transaction_date],
					   -- diff from inc. !!!! ----
					   cast(isnull(sum([Net_Revenue]),0) as numeric(20,2))															            [Net_Revenue_Life_Time],
					   isnull(count(distinct INVOICE_ID+STORE_ID),0)                                                                            [inv_count_Life_Time]
					   ---------------------------
				from [SuperPharmPoland].[dbo].[vW_Fact_Invoice]
				where LSClientID &gt; 0 and is_ecommerce = 0
				and [DOCUMENT_TYPE] &lt;&gt; 'ZWR'
				group by LSClientId
              ),
     kpi_2 as (
				select LSClientId, StoreParentId as [fav_store]
				from (
						select 
								LSClientId, S.StoreParentId
								,dense_rank()over(partition by LSClientId order by count(distinct a.STORE_ID+INVOICE_ID) desc ,sum(Net_Revenue) desc ,count(INVOICE_ROW_NUMBER) desc, max(INVOICE_DATE) desc) as rank_by_params
								,row_number()over(partition by LSClientId order by count(distinct a.STORE_ID+INVOICE_ID) desc ,sum(Net_Revenue) desc ,count(INVOICE_ROW_NUMBER) desc, max(INVOICE_DATE) desc) as row_num
						from [SuperPharmPoland].[dbo].[vW_Fact_Invoice] a
						join DWH_DIM_Stores s on s.Store_ID = a.STORE_ID
						where LSClientID &gt; 0 and is_ecommerce = 0
						and [DOCUMENT_TYPE] &lt;&gt; 'ZWR'
						group by LSClientId, S.StoreParentId
						) a
				where rank_by_params = 1 
				and row_num = 1
	          )
select isnull(a.LSClientId,b.LSClientId) LSClientId,
 [Net_Revenue_Current_Year]
,[Net_Revenue_Last_3M]
,[Net_Revenue_Last_6M]
,[inv_count_current_year]
,[inv_count_last_6M]
,[inv_count_last_3M]
,[Net_Revenue_Last_Year]
,[inv_count_last_year]
,[last_transaction_date]
-- diff from inc. !!!! ----
,[Net_Revenue_Life_Time]
,[inv_count_Life_Time]
---------------------------
,[fav_store]
from kpi_1 a 
full join kpi_2 b on a.LSClientId = b.LSClientId
;</property>
