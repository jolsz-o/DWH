                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- final 2
-- select only inv coupons
drop table if exists inv_coupons
;

select promotion_id
into inv_coupons
from DWH_dim_campaign_new
where promotion_type like '%paragon%'
;


-- select only raw coupons 
drop table if exists raw_coupons
;

select promotion_id
into raw_coupons
from DWH_dim_campaign_new
where promotion_type not like '%paragon%'
;


-- calc inv and raw coupons separatly and then sum them 
with inv_c as 
(
select -- calc only inv coupons
       [ls_id] as [Loyalty Id]

	  ,isnull(count(distinct prom_id),0)
	   as #_sent_cupons_lt

	  ,isnull(count(distinct case when year([coupon_sent_date]) = year(getdate()) then prom_id end),0)  
	   as [#_sent_cupons_current_year]

	  ,isnull(count(distinct case when year([coupon_sent_date]) = year(getdate())-1 then prom_id end),0)
	   as [#_sent_cupons_last_year]

	  ,isnull(count(distinct case when [coupon_sent_date]&gt;= dateadd(month,-6,getdate()) then prom_id end),0)
	   as [#_sent_cupons_last_6m]

	  ,isnull(count(distinct case when [coupon_sent_date]&gt;= dateadd(month,-3,getdate()) then prom_id end),0)
	   as [#_sent_cupons_last_3m]

	  ,isnull(count(distinct [invoice_id]),0)
	   as #_redemed_cupons_lt

	  ,isnull(count(distinct case when year([inv_date]) = year(getdate()) then [invoice_id] end),0)
	   as [#_redemed_cupons_current_year]

	  ,isnull(count(distinct case when year([inv_date]) = year(getdate())-1 then [invoice_id] end),0)
	   as [#_redemed_cupons_last_year]

	  ,isnull(count(distinct case when [inv_date]&gt;= dateadd(month,-6,getdate()) then [invoice_id] end),0)
	   as [#_redemed_cupons_last_6m]

	  ,isnull(count(distinct case when [inv_date]&gt;= dateadd(month,-3,getdate()) then [invoice_id] end),0) 
	   as [#_redemed_cupons_last_3m]
from (
		select
			   s.[prom_id]
			  ,s.[ls_id]
			  ,s.[coupon_sent_date]
			  ,s.[start_activation_date]
			  ,s.[end_activation_date]
			  ,a.[invoice_id]
			  ,a.store_id
			  ,a.inv_date
			  ,case when c.promotion_id is null then 0 else 1 end as is_inv_coupon
		from
			(select distinct 
				   [prom_id]
				  ,[ls_id]
				  ,[coupon_sent_date]
				  ,[start_activation_date]
				  ,[end_activation_date]
			from [superpharmpoland].[dbo].[cdp_dwh_fact_sent_coupons]) s	

		left join (
					SELECT 
							 a.[invoice_id]
							,a.[store_id]
							,a.[line_number]
							,a.[promotion_id]
							,a.[price_before_current_promotion_discount]
							,a.[promotion_discount]
							,a.[price_after_current_promotion_discount]
							,a.[item_id]
							,a.[inv_date]
							,a.[ls_kod]
							,ls.LoyaltyID as ls_id
					FROM [SuperPharmPoland].[dbo].[DWH_fact_activated_promotions] a
					left join [SuperPharmPoland].[dbo].[cdp_ODS_LS_customers_SapakimDB] ls
					on a.ls_kod = ls.kod_ls
					where a.ls_kod not in ('INNI','')
				   ) a
		on s.prom_id = a.promotion_id 
		and s.ls_id = a.ls_id 
		and cast(a.inv_date as date) &gt;= cast(s.start_activation_date as date) 
		and cast(a.inv_date as date) &lt;= cast(s.end_activation_date as date)
		left join inv_coupons c on s.prom_id = c.promotion_id
		) sent_activ
where is_inv_coupon = 1 
group by [ls_id]
),
    row_c as
(
select  --calc only raw coupons
       isnull([ls_id],0) 
	   as [Loyalty Id]

	  ,isnull(count(prom_id),0)
	   as #_sent_cupons_lt

	  ,isnull(count(case when year([coupon_sent_date]) = year(getdate()) then prom_id end),0)  
	   as [#_sent_cupons_current_year]

	  ,isnull(count(case when year([coupon_sent_date]) = year(getdate())-1 then prom_id end),0)
	   as [#_sent_cupons_last_year]

	  ,isnull(count(case when [coupon_sent_date]&gt;= dateadd(month,-6,getdate()) then prom_id end),0)
	   as [#_sent_cupons_last_6m]

	  ,isnull(count(case when [coupon_sent_date]&gt;= dateadd(month,-3,getdate()) then prom_id end),0)
	   as [#_sent_cupons_last_3m]

	  ,isnull(count([invoice_id]),0)
	   as #_redemed_cupons_lt

	  ,isnull(count(case when year([inv_date]) = year(getdate()) then [invoice_id] end),0)
	   as [#_redemed_cupons_current_year]

	  ,isnull(count(case when year([inv_date]) = year(getdate())-1 then [invoice_id] end),0)
	   as [#_redemed_cupons_last_year]

	  ,isnull(count(case when [inv_date]&gt;= dateadd(month,-6,getdate()) then [invoice_id] end),0)
	   as [#_redemed_cupons_last_6m]

	  ,isnull(count(case when [inv_date]&gt;= dateadd(month,-3,getdate()) then [invoice_id] end),0) 
	   as [#_redemed_cupons_last_3m]
from (
		select
			   s.[prom_id]
			  ,s.[ls_id]
			  ,s.[coupon_sent_date]
			  ,s.[start_activation_date]
			  ,s.[end_activation_date]
			  ,a.[invoice_id]
			  ,a.store_id
			  ,a.inv_date
			  ,case when c.promotion_id is null then 0 else 1 end as is_raw_coupon
		from
			(select distinct 
				   [prom_id]
				  ,[ls_id]
				  ,[coupon_sent_date]
				  ,[start_activation_date]
				  ,[end_activation_date]
			from [superpharmpoland].[dbo].[cdp_dwh_fact_sent_coupons]) s	
		left join (
					SELECT 
							 a.[invoice_id]
							,a.[store_id]
							,a.[line_number]
							,a.[promotion_id]
							,a.[price_before_current_promotion_discount]
							,a.[promotion_discount]
							,a.[price_after_current_promotion_discount]
							,a.[item_id]
							,a.[inv_date]
							,a.[ls_kod]
							,ls.LoyaltyID as ls_id
					FROM [SuperPharmPoland].[dbo].[DWH_fact_activated_promotions] a
					left join [SuperPharmPoland].[dbo].[cdp_ODS_LS_customers_SapakimDB] ls
					on a.ls_kod = ls.kod_ls
					where a.ls_kod not in ('INNI','')
				   ) a
		on s.prom_id = a.promotion_id 
		and s.ls_id = a.ls_id 
		and cast(a.inv_date as date) &gt;= cast(s.start_activation_date as date) 
		and cast(a.inv_date as date) &lt;= cast(s.end_activation_date as date)
		left join raw_coupons c on s.prom_id = c.promotion_id
		) sent_activ
where is_raw_coupon = 1 
group by [ls_id]
)
