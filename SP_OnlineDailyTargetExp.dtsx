          SQLTask:Connection="{2922D9E1-B054-4635-AFC5-68F26D34BB35}"
          SQLTask:SqlStatementSource="select CONVERT(smalldatetime,getdate(),112) dtDate,&#xA;&#x9;Case when &quot;[MEASURES].[storekey]&quot;='0' then 'All Stores' else &quot;[MEASURES].[storekey]&quot; end as StoreId,&#xA;&#x9;round(cast(&quot;[MEASURES].[Net Revenue]&quot; as float),0) as NetRev,&#xA;&#x9;round(cast(&quot;[MEASURES].[DailyTarget]&quot; as float),0) as DailyTarget,&#xA;&#x9;round(cast(&quot;[Measures].[Forecast to End Of Day]&quot; as float),0) as EODForecast,&#xA;&#x9;round(cast(&quot;[Measures].[PctOfDailyTargetForEOD]&quot; as float),2) as PctTargetOfEOD,&#xA;&#x9;round(cast(&quot;[Measures].[PctNetRevOfTarget]&quot; as float),2) as PctTargetOfNetRev&#xA;from openquery(online, '&#xA;With Member [MEASURES].[storekey] as [Stores].[Stores].currentmember.member_key&#xA;&#xA;Select &#xA;Non Empty {[MEASURES].[storekey],[Measures].[Net Revenue],[Measures].[DailyTarget],[Measures].[Forecast to End Of Day], [Measures].[PctOfDailyTargetForEOD], [Measures].[PctNetRevOfTarget]} On Columns ,&#xA;&#xA;Non Empty {[Stores].[Stores].members} On Rows &#xA;From [Daily Targets]')" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
