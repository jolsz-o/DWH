                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
      OPER_PROM.CENA as price_after_discount
     ,OPER_PROM.CENA_PRZED as price_before_discount
     ,OPER_PROM.ID_PROM as promotion_id
     ,OPER_PROM.ID_SKLEP as store_id
     ,OPER_PROM.NR_CEN as product_id
     ,OPER_PROM.NR_OPER as invoice_id
     ,OPER_PROM._ID_OPER_P  --???
     ,OPER_PROM.Korzysc as promotion_discount
     ,OPER_PROM.Lp as invoice_line 
     ,OPER_PROM.LiczbaAktywacji --??? Number_of_proms?
     ,OPER_PROM.BLOKADA --???
     ,OPER_PROM.ID 
from OPER_PROM (nolock) 
left join OPER (nolock) 
on  OPER_PROM.ID_SKLEP = OPER.ID_SKLEP
and OPER_PROM.NR_OPER = OPER.NR_OPER
where DATA_OP &gt;= convert(smalldatetime,getdate() -10,101)
;</property>
