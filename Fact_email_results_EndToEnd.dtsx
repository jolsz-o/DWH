                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select [FileName],[adres_email],[campaign_id],[campaign_name],[campaign_ship_date],[click_date],[link],[#_clicked_emails]
from
(select
       [FileName] 
      ,[adres_email] 
      ,[ID_kampanii] as [campaign_id]
      ,[nazwa_kampanii] as [campaign_name]
      ,[data_wysyłki_kampanii] as [campaign_ship_date]
      ,[data_kliknięcia] as [click_date] 
      ,[link]
	  ,count(1)over(partition by [adres_email],[id_kampanii]) as [#_clicked_emails]
	  ,row_number()over(partition by [adres_email],[id_kampanii] order by [data_wysyłki_kampanii]) as [row_number]
FROM [CDP].[dbo].[Redlink_email_clicks]) a
where [row_number] = 1</property>
