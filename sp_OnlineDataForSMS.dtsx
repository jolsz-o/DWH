      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{FE48FDC3-1CA7-4599-BBC6-97C5F8674FD4}"
          SQLTask:SqlStatementSource="Insert into dbo.STG_EODDataForSMS &#xA;select CONVERT(smalldatetime,getdate(),112) dtDate,&#xA; Case when &quot;[MEASURES].[storekey]&quot;='0' then 'All Stores' else &quot;[MEASURES].[storekey]&quot; end as StoreId,&#xA; round(cast(&quot;[MEASURES].[Net Revenue]&quot; as float),0) as Netto,&#xA; round(cast(&quot;[Measures].[GP]&quot; as float),2) as GP,&#xA; round(cast(&quot;[Measures].[Forecast to End Of Day]&quot; as float),0) as EOD&#xA;from openquery(online, '&#xA;With&#xA;&#xA;member [MEASURES].[storekey]&#xA;as [Stores].[Stores].currentmember.member_key&#xA;Member [Stores].[Stores].[SSS] as ([Stores].[Stores].[All Stores],[Identical Stores].[Identical Stores].[Identical Stores].&amp;[IdenticalStore])&#xA;Member [Stores].[Stores].[StoresWithoutOnline] as ([Stores].[Stores].[All Stores] - [Stores].[Stores].[Store Desc].&amp;[777])&#xA; &#xA; Select &#xA;Non Empty {[MEASURES].[storekey],[Measures].[Net Revenue],[Measures].[Forecast to End Of Day],[Measures].[GP] } On Columns ,&#xA;Non Empty {[Stores].[Stores].[SSS],[Stores].[Stores].[StoresWithoutOnline],[Stores].[Stores].members} On Rows &#xA;From [VSales]'&#xA; )" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
