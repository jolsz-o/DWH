                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
T.PLN_ID
,S.SECTION_ID
,F.FIXEL_ID
,P.POSITION_ID
,L.LOCATION_ID
--,TU.TOTAL_UNITS
,T.PRODUCT_ID
, T.SUM_TOTAL_UNITS   AS TOTAL_UNITS
,T.MAX_TOTAL_FACINGS  AS TOTAL_FACINGS  
, T.SUM_DEPTH_FACINGS AS DEPTH_FACINGS
, T.SUM_HORIZ_SPACE   AS HORIZ_SPACE

FROM
(
SELECT  C.[PLN_ID]
       ,C.[PRODUCT_ID]
	,D.MAX_TOTAL_FACINGS
,SUM(C.TOTAL_FACINGS) AS SUM_TOTAL_FACINGS
,SUM(C.[TOTAL_UNITS]) AS SUM_TOTAL_UNITS
,SUM(C.[DEPTH_FACINGS] ) AS SUM_DEPTH_FACINGS 
,SUM(C.[HORIZ_SPACE]) AS SUM_HORIZ_SPACE

 FROM [dbo].[ACN_POSITION] C

 INNER JOIN (
SELECT DISTINCT A.[PLN_ID]
               ,A.[PRODUCT_ID]
               ,B.MAX_TOTAL_FACINGS AS MAX_TOTAL_FACINGS
               FROM [dbo].[ACN_POSITION] A
INNER JOIN (
SELECT [PLN_ID]
      ,[PRODUCT_ID]
      ,MAX([TOTAL_FACINGS])  AS MAX_TOTAL_FACINGS
       FROM [dbo].[ACN_POSITION]
       GROUP BY [PLN_ID],[PRODUCT_ID]
	HAVING COUNT(*)&gt;1) B
	 
      ON A.PLN_ID=B.PLN_ID AND A.PRODUCT_ID=B.PRODUCT_ID
      WHERE A.[TOTAL_FACINGS]=B.MAX_TOTAL_FACINGS) D
	
	ON C.PLN_ID=D.PLN_ID AND C.PRODUCT_ID=D.PRODUCT_ID  
	GROUP BY   C.[PLN_ID],C.[PRODUCT_ID],D.MAX_TOTAL_FACINGS) T
	OUTER APPLY(SELECT TOP 1 [SECTION_ID]  FROM [dbo].[ACN_POSITION] WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID AND TOTAL_FACINGS=T.MAX_TOTAL_FACINGS)  S
	OUTER APPLY(SELECT TOP 1 [FIXEL_ID]    FROM [dbo].[ACN_POSITION] WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID AND TOTAL_FACINGS=T.MAX_TOTAL_FACINGS)  F
	OUTER APPLY(SELECT TOP 1 [POSITION_ID] FROM [dbo].[ACN_POSITION] WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID AND TOTAL_FACINGS=T.MAX_TOTAL_FACINGS)  P
	OUTER APPLY(SELECT TOP 1 [LOCATION_ID] FROM [dbo].[ACN_POSITION] WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID AND TOTAL_FACINGS=T.MAX_TOTAL_FACINGS)  L
	OUTER APPLY(SELECT TOP 1 [TOTAL_UNITS] FROM [dbo].[ACN_POSITION] WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID AND TOTAL_FACINGS=T.MAX_TOTAL_FACINGS)  TU
	
	

UNION ALL

SELECT 
[PLN_ID]
      ,S.[SECTION_ID]
      ,F.[FIXEL_ID]
      ,P.[POSITION_ID]
      ,L.[LOCATION_ID]
      ,[PRODUCT_ID]
      ,TU.[TOTAL_UNITS]
      ,[TOTAL_FACINGS]
      ,[DEPTH_FACINGS]   
,[HORIZ_SPACE]

FROM [dbo].[ACN_POSITION] T
OUTER APPLY(SELECT         [SECTION_ID]  FROM [dbo].[ACN_POSITION] WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID )  S
	OUTER APPLY(SELECT     [FIXEL_ID]    FROM [dbo].[ACN_POSITION] WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID )  F
	OUTER APPLY(SELECT     [POSITION_ID] FROM [dbo].[ACN_POSITION] WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID )  P
	OUTER APPLY(SELECT     [LOCATION_ID] FROM [dbo].[ACN_POSITION] WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID )  L
	OUTER APPLY(SELECT     [TOTAL_UNITS] FROM [dbo].[ACN_POSITION] WHERE [PLN_ID]=T.PLN_ID AND [PRODUCT_ID]=T.PRODUCT_ID )  TU
GROUP BY[PLN_ID]
      ,S.[SECTION_ID]
      ,F.[FIXEL_ID]
      ,P.[POSITION_ID]
      ,L.[LOCATION_ID]
      ,[PRODUCT_ID]
      ,TU.[TOTAL_UNITS]
      ,[TOTAL_FACINGS]
      ,[DEPTH_FACINGS]   
,[HORIZ_SPACE]
      
	HAVING COUNT(*)=1
 ORDER BY 

     T.PLN_ID
    ,T.PRODUCT_ID</property>
